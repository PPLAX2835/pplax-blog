-    private OAuth2AccessToken effectiveToken(String token) {
-        OAuth2AccessToken oAuth2AccessToken;
-        //解析token，使用tokenStore
-        try {
-            oAuth2AccessToken = tokenStore.readAccessToken(token);
-        } catch (Exception e) {
-            return null;
-        }
-        return oAuth2AccessToken;
-    }
