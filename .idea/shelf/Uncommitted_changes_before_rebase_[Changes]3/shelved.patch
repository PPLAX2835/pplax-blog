-            return null;
-        }
-
-        long uid = 0L;
-        try {
-            uid = Long.valueOf(correlationDataId);
-        } catch (NumberFormatException e) {
-            e.printStackTrace();
-            return null;
-        }
-
-        MessageLogVO messageLogVO = messageLogService.queryMessageLogByUid(uid);
-        return BeanUtils.copyProperties(messageLogVO, MessageLog.class);
-    }
-}
