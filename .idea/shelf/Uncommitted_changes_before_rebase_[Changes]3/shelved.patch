-        // 获取当前认证用户的角色集合
-        List<String> roleList = getRoleList();
-        List<String> roleCollect = roleList.stream()
-                .filter(stringList::contains)
-                .collect(Collectors.toList());
-        return !roleCollect.isEmpty();
-    }
