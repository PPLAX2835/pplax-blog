-    private String getUsernameFromRequest() {
-        HttpServletRequest request = PPLAXRequestUtils.getCurrentRequest();
-        return (String) request.getAttribute(RequestConstant.AUTH_SERVER_STORAGE_LOGIN_USERNAME_NAME);
-    }
